#%RAML 1.0
title: bestel-rest-api
baseUri: https://{host}/api/{version}
mediaType:
  - application/json
version: "1"
protocols:
  - HTTPS
  - HTTP
documentation:
  -
    content: "API perrona bien chingona y que chingue su madre el america "
    title: Home

/coverage/orbit:
  is:
    - hasRequestID
  get:
    displayName: ORBIT
    description: "Este endpoint devuelve la cobertura relacionada con una coordenada geogr√°fica."
    queryParameters:
      lat:
        type: number
        required: true
        example: 19.42851
      lng:
        type: number
        required: true
        example: -106.448250
    responses:
      400:
        body:
          application/json:
            example: !include responses/error/400.json
      "200":
        body:
          application/json:
            example: !include responses/orbit/coverageArray.json
            type: !include data-types/response/orbit/coverageResponseDT.raml







      